node {
  stage('SCM') {
    checkout scm
  }
  stage('SonarQube Analysis') {
    def mvn = tool 'maven';
    withSonarQubeEnv() {
      sh "${mvn}/bin/mvn clean verify sonar:sonar -Dsonar.projectKey=XiaoLFeng_schedule-planning-platform_af05b4d0-2540-4061-98f9-0ea54ac0f845 -Dsonar.projectName='schedule-planning-platform'"
    }
  }
}